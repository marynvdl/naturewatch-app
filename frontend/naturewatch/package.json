{
  "name": "NatureWatch",
  "description": "A geospatial web application using Vue3, Vuetify and Mapbox",
  "version": "0.1.0",
  "author": {
    "name": "Maryn",
    "url": "https://maryn.laarse.co.za/"
  },
  "homepage": "https://github.com/marynvdl/naturewatch-app",
  "repository": {
    "type": "git",
    "url": "git@github.com:marynvdl/naturewatch-app.git"
  },
  "bugs": {
    "url": "https://github.com/marynvdl/naturewatch-app/issues"
  },
  "engines": {
    "node": ">=18.14.2"
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check build-only",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "preview": "vite preview",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint  && prettier  \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "^1.4.3",
    "@mdi/font": "^7.4.47",
    "@turf/turf": "^7.0.0",
    "core-js": "^3.37.0",
    "mapbox-gl": "^3.3.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "posthog-js": "^1.103.1",
    "vue": "^3.3.9",
    "vue-router": "^4.3.2",
    "vuetify": "^3.5.14",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/types": "^7.24.7",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.6",
    "@types/mapbox__mapbox-gl-draw": "^1.4.6",
    "@types/mapbox-gl": "^3.1.0",
    "@types/node": "^20.14.0",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-c8": "^0.29.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-vuejs-accessibility": "^2.2.1",
    "eslint-plugin-vuetify": "^2.4.0",
    "eslint-plugin-yaml": "^0.5.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.2",
    "stylelint": "^16.5.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-checker": "0.6.4",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^0.29.2",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,yaml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3",
    "http-cache-semantics": "^4.1.1"
  },
  "stackblitz": {
    "startCommand": "yarn run test"
  }
}
